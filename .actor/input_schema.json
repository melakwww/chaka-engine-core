{
    "title": "Chaka Engine Input",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "firebase_key": {
            "title": "Firebase Service Account JSON",
            "type": "object",
            "description": "Paste the contents of your serviceAccountKey.json file here",
            "editor": "json",
            "isSecret": true
        },
        "start_cycles": {
            "title": "Starting Cycles",
            "type": "integer",
            "editor": "number",
            "default": 469244
        },
        "target_valuation": {
            "title": "Revenue Goal",
            "type": "string",
            "editor": "textfield",
            "default": "$10,000"
        }
    }
}